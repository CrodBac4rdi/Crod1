{
  "name": "crod-neural-framework",
  "version": "2.0.0",
  "description": "CROD Neural Framework - Advanced UI with UnoCSS",
  "scripts": {
    "build": "unocss --config uno.config.js '../lib/crod_web/**/*.*ex' --out-file css/uno.css --watch",
    "dev": "unocss --config uno.config.js '../lib/crod_web/**/*.*ex' --out-file css/uno.css --watch",
    "generate": "unocss --config uno.config.js '../lib/crod_web/**/*.*ex' --out-file css/uno.css",
    "deploy": "unocss --config uno.config.js '../lib/crod_web/**/*.*ex' --out-file css/uno.css --minify"
  },
  "devDependencies": {
    "@unocss/cli": "^0.58.0",
    "chart.js": "^4.5.0",
    "d3": "^7.9.0",
    "echarts": "^5.6.0",
    "plotly.js": "^2.29.0",
    "unocss": "^0.58.0"
  },
  "dependencies": {
    "phoenix": "file:../deps/phoenix",
    "phoenix_html": "file:../deps/phoenix_html",
    "phoenix_live_view": "file:../deps/phoenix_live_view"
  }
}
